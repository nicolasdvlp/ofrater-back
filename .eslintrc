{
    "env": {
      "browser": true,
      "es6": true
    },
    "extends": "standard",
    "rules": {
      "semi": ["error", "always"],
      "space-before-function-paren": ["error", {
        "anonymous": "never",
        "named": "never",
        "asyncArrow": "always"
      }],
      "quotes": ["error", "single"],
      "no-console": "off",
      "eqeqeq": "warn"
    }
}